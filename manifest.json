{
    "name": "Facebook messages remover",
    "description": "Remove all you messenger messeger on facebook",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "index.html"
    }
}